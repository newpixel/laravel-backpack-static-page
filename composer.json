{
    "name": "newpixel/laravel-backpack-static-page",
    "description": "Laravel Backpack Static Pages CRUD",
    "keywords": [
        "newpixel",
        "laravel-backpack-static-page"
    ],
    "homepage": "https://github.com/newpixel/laravel-backpack-static-page",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Catalin Prodan",
            "email": "catalin.prodan@newpixel.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "5.8.*",
        "backpack/crud": "3.5.*|3.6.*",
        "cviebrock/eloquent-sluggable": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Newpixel\\StaticPageCRUD\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Newpixel\\StaticPageCRUD\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Newpixel\\StaticPageCRUD\\StaticPageCRUDServiceProvider"
            ]
        }
    }
}
